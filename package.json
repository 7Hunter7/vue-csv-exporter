{
  "name": "vue-csv-exporter",
  "version": "1.0.0",
  "description": "Простой и мощный экспорт данных в CSV для Vue.js приложений. Без зависимостей, с поддержкой кириллицы и TypeScript.",
  "main": "dist/vue-csv-exporter.common.js",
  "module": "dist/vue-csv-exporter.esm.js",
  "unpkg": "dist/vue-csv-exporter.umd.min.js",
  "browser": "dist/vue-csv-exporter.umd.js",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:prod",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "prepublishOnly": "npm run test && npm run build",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "keywords": [
    "vue",
    "csv",
    "exporter",
    "export",
    "vuejs",
    "csv-exporter",
    "vue-csv",
    "russian",
    "cyrillic",
    "browser",
    "no-dependencies"
  ],
  "author": {
    "name": "Ivan Kalugin",
    "email": "i.am.hunter.777.ih@gmail.com",
    "url": "https://github.com/7Hunter7"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7Hunter7/vue-csv-exporter.git"
  },
  "bugs": {
    "url": "https://github.com/7Hunter7/vue-csv-exporter/issues"
  },
  "homepage": "https://github.com/7Hunter7/vue-csv-exporter#readme",
  "peerDependencies": {
    "vue": "^2.6.0 || ^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@vue/test-utils": "^1.3.6",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-vue": "^5.1.9",
    "standard-version": "^9.5.0",
    "vue": "^2.7.16",
    "vue-template-compiler": "^2.7.16"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "sideEffects": false
}
